{
  "dataflows": [
    {
      "name": "prueba-acceso",
      "inputs": [
        {
          "name": "test_input",
          "type": "file",
          "config": {
            "path": "data/test_input.csv",
            "format": "csv"
          },
          "spark_options": {
            "header": "true"
          }
        }
      ],
      "outputs": [
        {
          "name": "test_output",
          "type": "file",
          "input": "addPais",
          "config": {
            "path": "output/test_output.csv",
            "format": "csv",
            "save_mode": "overwrite"
          }
        }
      ],
      "transformations": [
        {
          "name": "filtroEdad",
          "type": "filter",
          "input": "test_input",
          "config": {
            "filter":"'edad > 30'"
          }
        },
        {
          "name": "addPais",
          "type": "add_fields",
          "input": "filtroEdad",
          "config": {
            "fields": [
              {
                "name": "pais",
                "expression": "Espa√±a"
              }
            ]
          }
        }
      ]
    }
  ]
}